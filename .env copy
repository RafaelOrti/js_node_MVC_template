DB_PORT=5432
DB_NAME=mydatabase
DB_USER=myuser
DB_PASSWORD=mypassword

# Configuración del servidor
PORT=3000

# Clave secreta para la generación de tokens JWT
JWT_SECRET=mysecretkey

# Configuración del servicio de correo electrónico
SMTP_HOST=smtp.example.com
